{
  "expo": {
    "name": "ticketyBoom",
    "slug": "ticketyBoom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/playstore.png",
    "scheme": "ticketyboom",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/playstore.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "preventAutoHide": true
        }
      ]
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/playstore.png"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
